#include<iostream>
using namespace std;
int main()
{
    int marks;
    cout<<"Enter your marks:";
    cin>>marks;
    if(marks>90)
    cout<<"EXCELLENT";
    if(marks>80 and marks<=90)
      cout<<"GOOD";
    if(marks>70 and marks<=80)
       cout<<"FAIR";
    if(marks>60 and marks<=70)
       cout<<"BELOW PAR";
    return 0;
    
}
